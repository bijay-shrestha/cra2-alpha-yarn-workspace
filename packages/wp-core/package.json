{
  "name": "wp-core",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
  },
  "devDependencies": {
  },
  "scripts": {
    "build:src": "babel --config-file ../../babel.config.js ./src --out-dir ./dist --delete-dir-on-start --copy-files",
    "build": "cross-env NODE_ENV=production yarn build:src",
    "start": "cross-env NODE_ENV=development yarn build:src --watch --source-maps",
    "format": "prettier --write src/**/*.{js,jsx,json,css}",
    "lint": "eslint src/**/*.js"
  }
}
